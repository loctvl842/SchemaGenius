{
  "name": "SchemaGenius",
  "displayName": "SchemaGenius",
  "description": "SchemaGenius: Seamlessly convert your DBML designs from dbdiagram into ORM code within VS Code.",
  "publisher": "Rockship-loctvl842",
  "repository": "https://github.com/loctvl842/SchemaGenius",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:schemagenius.generateSchema"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "schemagenius.generateSchema",
        "title": "Generate Schema"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@dbml/core": "^2.6.1",
    "chalk": "4.1.2",
    "inflection": "^3.0.0",
    "lodash": "^4.17.21",
    "pkg-dir": "^7.0.0"
  }
}
